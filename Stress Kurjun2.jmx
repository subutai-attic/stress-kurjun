          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Generate Key pair" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import ai.subut.kurjun.security.manager.utils.pgp.*;
import java.util.UUID;
import org.bouncycastle.openpgp.*;

UUID uuid = UUID.randomUUID();
String keyId = uuid.toString() + &quot;@mail.com&quot;;
KeyPair keyPair = PGPEncryptionUtil.generateKeyPair(keyId, &quot;&quot;, false);

String privateKey = PGPEncryptionUtil.armorByteArrayToString( keyPair.getSecKeyring() );
String publicKey = PGPEncryptionUtil.armorByteArrayToString( keyPair.getPubKeyring() );


vars.put(&quot;privateKey&quot;, privateKey);
vars.put(&quot;publicKey&quot;, publicKey);
vars.put(&quot;keyId&quot;, keyId);

log.info(keyId);</stringProp>
          </BeanShellPreProcessor>
