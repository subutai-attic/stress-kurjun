          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Sign auth_id" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import ai.subut.kurjun.security.manager.utils.pgp.*;
import java.util.UUID;
import org.bouncycastle.openpgp.*;

String privateKey = vars.get(&quot;privateKey&quot;);
String publicKey = vars.get(&quot;publicKey&quot;);

PGPSecretKeyRing secRing = PGPKeyUtil.readSecretKeyRing( privateKey );
PGPPublicKeyRing pubRing = PGPKeyUtil.readPublicKeyRing( publicKey );

PGPSecretKey secretKey = secRing.getSecretKey();

String authId = prev.getResponseDataAsString() + &quot;\r\n&quot;;
byte[] signedMessageArmor = PGPEncryptionUtil.clearSign( authId.getBytes(), secretKey, &quot;&quot;.toCharArray(), &quot;&quot; );
String signedMessage = new String( signedMessageArmor, &quot;UTF-8&quot; );

String fingerprint = PGPKeyUtil.getFingerprint( pubRing.getPublicKey().getFingerprint() );

vars.put(&quot;message&quot;, signedMessage);
vars.put(&quot;fingerprint&quot;, fingerprint);</stringProp>
          </BeanShellPostProcessor>
